{
  "name": "crop-tool-example",
  "version": "1.0.0",
  "description": "A sample application using react-image-crop-tool node module",
  "main": "index.js",
  "scripts": {
    "start": "npm run production",
    "production": "rm -rf public/index.html && NODE_ENV=production webpack -p && NODE_ENV=production node app.js",
    "webpack-dev-server": "NODE_ENV=development PORT=8080 webpack-dev-server --content-base public/ --hot --inline --devtool inline-source-map --history-api-fallback",
    "development": "cp views/index.html public/index.html && NODE_ENV=development webpack && npm run webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jihdeh/react-crop-example.git"
  },
  "keywords": [
    "React",
    "crop",
    "tool",
    "image",
    "npm"
  ],
  "author": "Jihdeh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jihdeh/react-crop-example/issues"
  },
  "homepage": "https://github.com/jihdeh/react-crop-example#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "cosmicjs": "^2.1.2",
    "events": "^1.1.0",
    "express": "^4.14.0",
    "flux": "^2.1.1",
    "history": "^3.0.0",
    "hogan-express": "^0.5.2",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-image-select-component": "^1.1.2",
    "react-router": "^2.4.1",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "lodash": "^4.13.1",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.1"
  }
}
